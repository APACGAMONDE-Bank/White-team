<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="126" android:keyLabel="~" android:keyEdgeFlags="left" android:keyWidth="5%p"/>
        <Key android:keyLabel="1"/>
        <Key android:keyLabel="2"/>
        <Key android:keyLabel="3"/>
        <Key android:keyLabel="4"/>
        <Key android:keyLabel="5"/>
        <Key android:keyLabel="6"/>
        <Key android:keyLabel="7"/>
        <Key android:keyLabel="8"/>
        <Key android:keyLabel="9"/>
        <Key android:keyLabel="0"/>
        <Key android:keyLabel="-"/>
        <Key android:keyLabel="="/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:isRepeatable="true" android:keyEdgeFlags="right" android:keyWidth="15%p"/>
    </Row>
    <Row>
        <Key android:codes="09" android:keyLabel="tab" android:keyEdgeFlags="left" android:keyWidth="15%p"/>

        <Key android:keyLabel="й"/>
        <Key android:keyLabel="ц"/>
        <Key android:keyLabel="у"/>
        <Key android:keyLabel="к"/>
        <Key android:keyLabel="е"/>
        <Key android:keyLabel="н"/>
        <Key android:keyLabel="г"/>
        <Key android:keyLabel="ш"/>
        <Key android:keyLabel="щ"/>
        <Key android:keyLabel="з"/>
        <Key android:keyLabel="х"/>
        <Key android:keyLabel="ъ"/>
        <Key android:keyLabel="\\" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="113" android:keyLabel="ctrl" android:keyEdgeFlags="left" android:keyWidth="18%p"/>

        <Key android:keyLabel="ф" />
        <Key android:keyLabel="ы"/>
        <Key android:keyLabel="в"/>
        <Key android:keyLabel="а"/>
        <Key android:keyLabel="п"/>
        <Key android:keyLabel="р"/>
        <Key android:keyLabel="о"/>
        <Key android:keyLabel="л"/>
        <Key android:keyLabel="д" />
        <Key android:keyLabel="ж"/>
        <Key android:keyLabel="э"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="18%p" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift"
            android:keyWidth="15%p" android:isModifier="true"
            android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:keyLabel="я"/>
        <Key android:keyLabel="ч"/>
        <Key android:keyLabel="с"/>
        <Key android:keyLabel="м"/>
        <Key android:keyLabel="и"/>
        <Key android:keyLabel="т"/>
        <Key android:keyLabel="ь"/>
        <Key android:keyLabel="б" />
        <Key android:keyLabel="ю'"/>
        <Key android:codes="19" android:isRepeatable="true" android:keyLabel="↑"/>
        <Key android:codes="126" android:keyLabel="~" android:keyEdgeFlags="right"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-319" android:keyLabel="Esc" android:keyWidth="15%p" android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyLabel="ENG" android:keyWidth="15%p"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="30%p" android:isRepeatable="true"/>
        <Key android:codes="-320" android:keyLabel="Cp"/>
        <Key android:codes="-321" android:keyLabel="Ps"/>
        <Key android:codes="21" android:isRepeatable="true" android:keyLabel="←"/>
        <Key android:codes="20" android:isRepeatable="true" android:keyLabel="↓"/>
        <Key android:codes="22" android:isRepeatable="true" android:keyLabel="→" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    